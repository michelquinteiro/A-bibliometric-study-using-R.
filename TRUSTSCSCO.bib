@ARTICLE{Qian2020,
author={Qian, J. and Wu, W. and Yu, Q. and Ruiz-Garcia, L. and Xiang, Y. and Jiang, L. and Shi, Y. and Duan, Y. and Yang, P.},
title={Filling the trust gap of food safety in food trade between the EU and China: An interconnected conceptual traceability framework based on blockchain},
