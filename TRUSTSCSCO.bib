@ARTICLE{Love2020,
author={Love, D.C. and Lane, R.M. and Kuehl, L.M. and Hudson, B. and Harding, J. and Clancy, K. and Fry, J.P.},
title={Performance and conduct of supply chains for United States farmed oysters},
