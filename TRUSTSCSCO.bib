@ARTICLE{vanderMerwe2017101,
author={van der Merwe, M. and Kirsten, J.F. and Trienekens, J.H.},
title={Information sharing as a safeguard against the opportunistic behavior of South African Karoo Lamb farmers},
journal={Agricultural Economics (United Kingdom)},
year={2017},
