@ARTICLE{Yang2019323,
author={Yang, X. and Wang, M. and Xu, D. and Luo, N. and Sun, C.},
title={Data storage and query method of agricultural products traceability information based on blockchain [基于区块链的农产品追溯系统信息存储模型与查询方法]},
