title={Postharvest practices along supply chains of Solanum aethiopicum (Shum) and Amaranthus lividus (Linn) leafy vegetables in Wakiso and Kampala Districts, Uganda},
journal={African Journal of Food, Agriculture, Nutrition and Development},
year={2020},
volume={20},
number={2},
