@inproceedings{ ISI:000402648800125,
Author = {Jarosz-Angowska, Aneta and Angowski, Marek and Kijek, Tomasz},
Editor = {{Raupeliene, A}},
Title = {{THE CREATION OF SOCIAL CAPITAL IN RURAL AREAS IN THE LUBELSKIE REGION}},
Booktitle = {{7TH INTERNATIONAL SCIENTIFIC CONFERENCE RURAL DEVELOPMENT 2015: TOWARDS
   THE TRANSFER OF KNOWLEDGE, INNOVATIONS AND SOCIAL PROGRESS}},
