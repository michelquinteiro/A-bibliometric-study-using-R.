Title = {{Social capital on poultry farms in South Sulawesi, Indonesia}},
Booktitle = {{1ST INTERNATIONAL CONFERENCE ON FOOD SECURITY AND SUSTAINABLE
   AGRICULTURE IN THE TROPICS (IC-FSSAT)}},
Series = {{IOP Conference Series-Earth and Environmental Science}},
