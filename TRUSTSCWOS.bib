@article{ ISI:000353737300010,
Author = {Grebitus, Carola and Steiner, Bodo and Veeman, Michele},
Title = {{The roles of human values and generalized trust on stated preferences
   when food is labeled with environmental footprints: Insights from
   Germany}},
