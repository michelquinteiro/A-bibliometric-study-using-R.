@article{ ISI:000390590500018,
Author = {Harris, David James and Rosado, Daniela and Xavier, Raquel},
Title = {{DNA Barcoding Reveals Extensive Mislabeling in Seafood Sold in
   Portuguese Supermarkets}},
Journal = {{JOURNAL OF AQUATIC FOOD PRODUCT TECHNOLOGY}},
Year = {{2016}},
Volume = {{25}},
Number = {{8}},
Pages = {{1375-1380}},
Abstract = {{Seafood mislabeling is a widespread problem even in Europe where the
   food market is highly regulated. In the present study, we analyzed the
   accuracy of seafood labeling in five Portuguese supermarket chains
   through DNA barcode. Our results show that approximately 19\% of
   examined samples were mislabeled and that the consumer's trust is
   misplaced. As mislabeling was greater in invertebrate species, we
   recommend that future studies led by government agencies and scientists
   aiming to test labeling accuracy should expand from finfish to include
   invertebrates such as crustaceans and bivalves.}},
