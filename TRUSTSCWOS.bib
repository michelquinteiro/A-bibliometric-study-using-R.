@article{ ISI:000405992300327,
Author = {Li, Ming and Fang, Yuan and Liu, Jun},
Title = {{Research on Smart Mobile Terminal Security Technology Based on Trusted
   Computing}},
Journal = {{AGRO FOOD INDUSTRY HI-TECH}},
Year = {{2017}},
Volume = {{28}},
Number = {{1}},
Pages = {{3278-3281}},
Month = {{JAN-FEB}},
Abstract = {{Smart mobile terminals need to be connected to a network, and to solve
   security threats in complex network environments through trusted
   computing. In this paper, starting from the drawbacks of traditional
   smart mobile terminal security technologies, given the principle of
   trusted computing, roots of trust dedicated to trusted computing are
   built through the computer platform; reliability of roots of trust is
   mainly guaranteed by system physical security and management. In the
   actual visit, it is necessary to validly measure all entities. On this
   basis, this paper constructs a smart mobile terminal security system
   model based on trusted computing, which can realize such features as
   trust transfer structure, authentication, storage confidentiality and
   trusted network connect. It is visible that trusted computing is
   suitable for the construction of smart mobile terminal security systems.}},
