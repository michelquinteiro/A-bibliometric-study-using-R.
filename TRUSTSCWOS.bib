@inproceedings{ ISI:000378599800011,
Author = {Benge, J. and Barber, A. and McCusker, K. and Le Quellec, I. and
   Manhire, J. and Hunt, L. and Moller, H. and Rosin, C. and MacLeod, C. J.},
Editor = {{Batt, PJ}},
Title = {{The New Zealand sustainability dashboard: connecting growers, industry,
   consumers, regulators and policy makers to drive sustainable
   horticulture}},
Booktitle = {{XXIX INT HORTICULTURAL CONGRESS ON HORTICULTURE: SUSTAINING LIVES,
   LIVELIHOODS AND LANDSCAPES: XVII INT SYMP ON HORTICULTURAL ECONOMICS AND
   MANAGEMENT AND V INT SYMP ON IMPROVING THE PERFORMANCE OF SUPPLY CHAINS
   IN THE TRANSITIONAL ECONOMIES}},
Series = {{Acta Horticulturae}},
Year = {{2015}},
Volume = {{1103}},
Pages = {{69-73}},
Note = {{29th Int Horticultural Congress on Horticulture - Sustaining Lives,
   Livelihoods and Landscapes (IHC) / 17th Int Symposium on Horticultural
   Econ and Management / 5th Int Symposium on Improving the Performance of
   Supply Chains in the Transit Economies, Brisbane, AUSTRALIA, AUG 17-22,
   2014}},
Organization = {{Int Soc Horticultural Sci}},
Abstract = {{The New Zealand Sustainability Dashboard (NZSD) is an on-line assessment
   and reporting tool to guide sustainable production of wine and
   kiwifruit. It uses internationally recognised frameworks and locally
   grounded key performance indicators (KPIs) to simultaneously enable
   learning for sustainability, meet compliance requirements and verify
   environmental, social, production and business performance. Most KPIs
   are regularly scored and uploaded by the growers and viticulturists so
   that participation is inexpensive, rapid and does not require specialist
   knowledge. Some secondary or aggregate KPIs will be automatically
   calculated by accessing national and regional databases and maps (e.g.
   weather and soil maps) or models (e.g. a carbon footprint calculator).
   The grower's KPIs are instantly benchmarked against neighbours'
   performance and matched enterprises to incentivise improvement, and
   compared against their own previous scores for trend analysis. KPIs will
   be scaled against amber and red alert thresholds to flag when
   intervention is required. `What-if' decision modules will be linked to
   the KPIs so that the growers can optimise investments to improve
   performance. Survey modules will allow the network of growers to guide
   industry strategy. Key challenges and opportunities encountered when
   designing and testing the prototype New Zealand Sustainability
   Dashboards are balancing generalisability and specificity, scientific
   reliability, cross-scale linkage and participation by growers. The
   Sustainability Dashboard needs to be trusted by multiple stakeholders if
   it is to secure food integrity, food safety, environmental stewardship
   and future-proof market access by verifying market assurance claims.}},
