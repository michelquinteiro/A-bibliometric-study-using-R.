@article{ ISI:000472696600014,
Author = {Banovic, Marija and Reinders, Machiel J. and Claret, Anna and Guerrero,
   Luis and Krystallis, Athanasios},
Title = {{``One Fish, Two Fish, Red Fish, Blue Fish{''}: How ethical beliefs
   influence consumer perceptions of ``blue{''} aquaculture products?}},
Journal = {{FOOD QUALITY AND PREFERENCE}},
Year = {{2019}},
Volume = {{77}},
Pages = {{147-158}},
Month = {{OCT}},
Abstract = {{Respecting ethical beliefs of consumers is an important precondition for
   food manufacturers in their attempt to improve their positioning in the
   European food market. Based on a cross-cultural survey of 2511 European
   participants, this research demonstrates how ethical beliefs affect
   consumer perceptions of ``blue{''} (i.e. environmentally friendly)
   aquaculture products. The study further emphasises that the positive
   effect of ethical beliefs on purchase intention operates via an indirect
   route mediated by consumers' trust in a product category. Consumer
   involvement has limited moderation effect on the above relationships. To
   expand its ``blue{''} business, a key policy recommendation to
   aquaculture product manufacturers and policy makers is to urge stable
   and reliable standards of control in environmentally responsible
   aquaculture production so that consumers can rely on the information
   source and increase their trust in aquaculture products.}},
